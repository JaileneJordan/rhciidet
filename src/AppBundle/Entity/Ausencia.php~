<?php

namespace AppBundle\Entity;

/**
 * Ausencia
 */
class Ausencia
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $constancia;

    /**
     * @var \DateTime
     */
    private $fechaInicial;

    /**
     * @var \DateTime
     */
    private $fechaFinal;

    /**
     * @var integer
     */
    private $totalDias;

    /**
     * @var \DateTime
     */
    private $fechagobfed;

    /**
     * @var \DateTime
     */
    private $fechasep;

    /**
     * @var \DateTime
     */
    private $fechatec;

    /**
     * @var \DateTime
     */
    private $fechaciidet;

    /**
     * @var \AppBundle\Entity\Licencia
     */
    private $idLicencia;

    /**
     * @var \AppBundle\Entity\Empleado
     */
    private $idEmpleado;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set constancia
     *
     * @param string $constancia
     *
     * @return Ausencia
     */
    public function setConstancia($constancia)
    {
        $this->constancia = $constancia;

        return $this;
    }

    /**
     * Get constancia
     *
     * @return string
     */
    public function getConstancia()
    {
        return $this->constancia;
    }

    /**
     * Set fechaInicial
     *
     * @param \DateTime $fechaInicial
     *
     * @return Ausencia
     */
    public function setFechaInicial($fechaInicial)
    {
        $this->fechaInicial = $fechaInicial;

        return $this;
    }

    /**
     * Get fechaInicial
     *
     * @return \DateTime
     */
    public function getFechaInicial()
    {
        return $this->fechaInicial;
    }

    /**
     * Set fechaFinal
     *
     * @param \DateTime $fechaFinal
     *
     * @return Ausencia
     */
    public function setFechaFinal($fechaFinal)
    {
        $this->fechaFinal = $fechaFinal;

        return $this;
    }

    /**
     * Get fechaFinal
     *
     * @return \DateTime
     */
    public function getFechaFinal()
    {
        return $this->fechaFinal;
    }

    /**
     * Set totalDias
     *
     * @param integer $totalDias
     *
     * @return Ausencia
     */
    public function setTotalDias($totalDias)
    {
        $this->totalDias = $totalDias;

        return $this;
    }

    /**
     * Get totalDias
     *
     * @return integer
     */
    public function getTotalDias()
    {
        return $this->totalDias;
    }

    /**
     * Set fechagobfed
     *
     * @param \DateTime $fechagobfed
     *
     * @return Ausencia
     */
    public function setFechagobfed($fechagobfed)
    {
        $this->fechagobfed = $fechagobfed;

        return $this;
    }

    /**
     * Get fechagobfed
     *
     * @return \DateTime
     */
    public function getFechagobfed()
    {
        return $this->fechagobfed;
    }

    /**
     * Set fechasep
     *
     * @param \DateTime $fechasep
     *
     * @return Ausencia
     */
    public function setFechasep($fechasep)
    {
        $this->fechasep = $fechasep;

        return $this;
    }

    /**
     * Get fechasep
     *
     * @return \DateTime
     */
    public function getFechasep()
    {
        return $this->fechasep;
    }

    /**
     * Set fechatec
     *
     * @param \DateTime $fechatec
     *
     * @return Ausencia
     */
    public function setFechatec($fechatec)
    {
        $this->fechatec = $fechatec;

        return $this;
    }

    /**
     * Get fechatec
     *
     * @return \DateTime
     */
    public function getFechatec()
    {
        return $this->fechatec;
    }

    /**
     * Set fechaciidet
     *
     * @param \DateTime $fechaciidet
     *
     * @return Ausencia
     */
    public function setFechaciidet($fechaciidet)
    {
        $this->fechaciidet = $fechaciidet;

        return $this;
    }

    /**
     * Get fechaciidet
     *
     * @return \DateTime
     */
    public function getFechaciidet()
    {
        return $this->fechaciidet;
    }

    /**
     * Set idLicencia
     *
     * @param \AppBundle\Entity\Licencia $idLicencia
     *
     * @return Ausencia
     */
    public function setIdLicencia(\AppBundle\Entity\Licencia $idLicencia = null)
    {
        $this->idLicencia = $idLicencia;

        return $this;
    }

    /**
     * Get idLicencia
     *
     * @return \AppBundle\Entity\Licencia
     */
    public function getIdLicencia()
    {
        return $this->idLicencia;
    }

    /**
     * Set idEmpleado
     *
     * @param \AppBundle\Entity\Empleado $idEmpleado
     *
     * @return Ausencia
     */
    public function setIdEmpleado(\AppBundle\Entity\Empleado $idEmpleado = null)
    {
        $this->idEmpleado = $idEmpleado;

        return $this;
    }

    /**
     * Get idEmpleado
     *
     * @return \AppBundle\Entity\Empleado
     */
    public function getIdEmpleado()
    {
        return $this->idEmpleado;
    }
}
